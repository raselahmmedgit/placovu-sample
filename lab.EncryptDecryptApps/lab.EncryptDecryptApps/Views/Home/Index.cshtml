<style type="text/css">
    /*Country Code*/
    .phone-control {
        position: relative;
        display: table;
        border-collapse: separate;
        margin-bottom: 15px;
        border-radius: 4px;
        border: 1px solid #ccc;
        border-radius: 4px;
        -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075);
        box-shadow: inset 0 1px 1px rgba(0,0,0,.075);
    }

        .phone-control.border-left {
            border-left: 2px solid #428bca !important;
        }

        .phone-control.focused {
            border: 1px solid #999;
        }

    .country-control {
        min-width: 39px;
        width: 1%;
        white-space: nowrap;
        vertical-align: middle;
        font-size: 14px;
        font-weight: 400;
        line-height: 1;
        color: #555;
        text-align: center;
        border-radius: 4px;
        display: table-cell;
    }

        .country-control button {
            background: #FFF !important;
            border: none !important;
            height: 32px;
            border-radius: 0px !important;
        }

    .phone-control .country-control .dropdown-toggle {
        box-shadow: none;
    }

    .number-code-control {
        min-width: 30px;
        width: 1%;
        white-space: nowrap;
        vertical-align: middle;
        display: table-cell;
        line-height: 1;
        background: #fff;
        height: 32px;
        border-radius: 0px !important;
        font-size: 14px;
    }

    .number-control {
        position: relative;
        z-index: 2;
        float: left;
        width: 100%;
        margin-bottom: 0;
        display: table-cell;
        font-size: 14px;
        font-weight: normal;
        color: #333;
        background-color: #fff;
        -webkit-box-shadow: none;
        box-shadow: none;
        height: 32px;
        border-radius: 0px !important;
        padding-left: 5px;
    }

    .phone-control input.number-control {
        border: none;
    }

    .phone-control .country-flag > span {
        margin-right: 5px;
    }

    .number-control:focus {
        border-color: #999;
        outline: 0;
        -webkit-box-shadow: none;
        box-shadow: none;
    }

    .country-code-dropdown {
        margin-top: -1px;
    }

        .country-code-dropdown ul {
            margin: 0px;
            padding: 0px;
            float: left;
            width: 100%;
        }

        .country-code-dropdown li {
            list-style-type: none;
            padding: 10px 10px !important;
            text-align: left;
            min-width: 260px !important;
        }

            .country-code-dropdown li a {
                text-decoration: none;
                padding: 5px 0px !important;
            }

            .country-code-dropdown li.active {
                background: #41739C;
            }
    /*Country Code*/

    .trSortablePlaceholder {
        cursor: move;
    }
</style>

