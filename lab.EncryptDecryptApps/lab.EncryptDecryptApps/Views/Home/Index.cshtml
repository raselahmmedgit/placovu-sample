    window.ontrackUtility = (function () {

        var ontrackUtility = {};

        function countryPhoneCodeDropDownMask(eid, countryiso) {

            var numbercontrol = "input[id='" + eid + "']";

            if (countryiso !== undefined && countryiso != '') {
                if (countryiso.toLowerCase() == 'us' || countryiso.toLowerCase() == 'ca') {
                    $(numbercontrol).mask("000-000 0000", { 'translation': { 0: { pattern: /[0-9]/ } } });
                }
                else {
                    $(numbercontrol).mask("000000000000000", { 'translation': { 0: { pattern: /[0-9]/ } } });
                }
            }
            else {
                $(numbercontrol).mask("000-000 0000", { 'translation': { 0: { pattern: /[0-9]/ } } });
            }
        };

        function countryPhoneCodeDropDownValidationList(form, phoneCodeIds) {
            if (typeof phoneCodeIds !== 'undefined' && phoneCodeIds.length > 0) {
                for (i = 0; i < phoneCodeIds.length; i++) {
                    var phoneCodeId = phoneCodeIds[i];
                    return countryPhoneCodeDropDownValidation(form, phoneCodeId)
                }
            }
        }

        function countryPhoneCodeDropDownValidation(form, phoneCodeId) {
            if (typeof form !== 'undefined' && form.length > 0) {
                var phoneNumber = form.find("#" + phoneCodeId);
                if (typeof phoneNumber !== 'undefined' && phoneNumber.length > 0) {
                    var phoneNumberId = phoneNumber.attr('id');
                    var phoneNumberValue = phoneNumber.val();
                    phoneNumberValue = phoneNumberValue.replace('-', '');
                    phoneNumberValue = phoneNumberValue.replace(' ', '');

                    phoneNumberErrorMsg = phoneNumberId.replace('Phone', '');

                    var isValid = false;
                    if (phoneNumberValue.length >= 10) {
                        isValid = true;
                    }
                    else {
                        isValid = false;
                    }

                    var phoneNumberErrorSpan = form.find("span[data-valmsg-for='" + phoneNumberId + "']");

                    if (isValid) {

                        phoneNumber.removeClass('input-validation-error');
                        phoneNumber.addClass('valid');
                        phoneNumber.attr("aria-invalid", "false");

                        phoneNumberErrorSpan.removeClass('field-validation-error');
                        phoneNumberErrorSpan.addClass('field-validation-valid');

                        phoneNumberErrorSpan.html("");
                        return true;

                    } else {

                        var phoneNumberErrorMsg = "<span id='" + phoneNumberId + "-error'>The " + phoneNumberErrorMsg + " Phone field is not a valid phone number.</span>";
                        phoneNumber.removeClass('valid');
                        phoneNumber.addClass('input-validation-error');
                        phoneNumber.attr("aria-invalid", "true");

                        phoneNumberErrorSpan.removeClass('field-validation-valid');
                        phoneNumberErrorSpan.addClass('field-validation-error');

                        phoneNumberErrorSpan.html(phoneNumberErrorMsg);
                        return false;

                    }

                    return false;
                }
            }
        }

        ontrackUtility.initCountryPhoneCodeDropDownSelectedCountryIso = function (eid, selectedcountryiso) {
            countryPhoneCodeDropDownMask(eid, selectedcountryiso);
        };

        ontrackUtility.initCountryPhoneCodeDropDown = function () {

            $('.country-code-dropdown ul li a').click(function () {
                var flag = $(this).find('span.country-flag span').attr('class');
                var countryphonecode = $(this).find('.country-phone-code').text();
                $(this).closest('.country-control').removeClass('open');
                $(this).closest('.country-control').find('.selected-country-flag').attr('class', 'selected-country-flag ' + flag);
                $(this).closest('.country-control').next('.number-code-control').text(countryphonecode);
                $('.country-code-dropdown input').val('').focus();
                $(this).closest('.country-control').parent().find('input.number-control').focus();
                var id = $(this).closest('.country-control').data('id');
                $("#" + id).val(countryphonecode);
            });

        };

        ontrackUtility.initCountryPhoneCodeDropDownValidation = function (formId, phoneCodeIds) {

            var form = $("form#" + formId);

            if (typeof form !== 'undefined' && form.length > 0) {
                var submitButtonId = 'form#' + formId + ' input[type=submit]';
                $('body').on('click', submitButtonId, function (e) {
                    var isValid = countryPhoneCodeDropDownValidationList(form, phoneCodeIds);
                    return isValid;
                });
            }

        };

        return ontrackUtility;

