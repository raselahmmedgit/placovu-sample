    function PhoneNumberValidation(form, id)
    {
        if (typeof form !== 'undefined' && form.length > 0) {
            var phoneNumber = form.find("#" + id);
            if (typeof phoneNumber !== 'undefined' && phoneNumber.length > 0) {
                var phoneNumberId = phoneNumber.attr('id');
                var phoneNumberValue = phoneNumber.val();
                phoneNumberValue = phoneNumberValue.replace('-', '');
                phoneNumberValue = phoneNumberValue.replace(' ', '');

                phoneNumberErrorMsg = phoneNumberId.replace('Phone', '');

                var isValid = false;
                if (phoneNumberValue.length == 10) {
                    isValid = true;
                }
                else {
                    isValid = false;
                }

                var phoneNumberErrorSpan = form.find("span[data-valmsg-for='" + phoneNumberId + "']");
                var phoneNumberErrorMsg = "<span id='" + phoneNumberId + "-error'>The " + phoneNumberErrorMsg + " Phone field is not a valid phone number.</span>";

                if (isValid) {
                    phoneNumber.removeClass('input-validation-error');
                    phoneNumber.addClass('valid');
                    phoneNumber.attr("aria-invalid", "false");

                    phoneNumberErrorSpan.removeClass('field-validation-error');
                    phoneNumberErrorSpan.addClass('field-validation-valid');

                    phoneNumberErrorSpan.html("");
                    return true;
                } else {
                    phoneNumber.removeClass('valid');
                    phoneNumber.addClass('input-validation-error');
                    phoneNumber.attr("aria-invalid", "true");

                    phoneNumberErrorSpan.removeClass('field-validation-valid');
                    phoneNumberErrorSpan.addClass('field-validation-error');

                    phoneNumberErrorSpan.html(phoneNumberErrorMsg);
                    return false;
                }

                return false;
            }
        }
    }

